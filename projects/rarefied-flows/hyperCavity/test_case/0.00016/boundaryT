FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.00016";
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 300;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 300;
    }
    wall
    {
        type            fixedValue;
        value           uniform 550;
    }
    frontAndBack
    {
        type            empty;
    }
}


